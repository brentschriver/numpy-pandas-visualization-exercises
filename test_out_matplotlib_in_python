
import matplotlib.pyplot as plt  
import numpy as np
import math
from random import randint
import random
import pandas as pd

x = [1,2,3,4,5,6,7]
y = [100/x for x in x]

plt.plot(x, y, 'r-', ls = '--', marker = 'x', ms = '5', label = 'some function')
plt.title('My first matplotlib figure')
plt.xlabel('x')
plt.ylabel('y')
plt.grid('True', ls = '--')
plt.xlim(0,7)
plt.ylim(0,100)